<div class="page page-b2c-home">
    <div class="cookiealertcontainer active">{{# cookiealert }}
        <div><h2 class="{{styleModifier}}">{{heading}}</h2>
            <div class="cookiedescription">
                <p class="{{styleModifier}}">{{ description }}</p> {{# cookieAlertCloseButton
                }}
                <button class="btn {{ styleModifier }}" type="button" name="{{ buttonName }}">{{ buttonText }}</button>
                {{/ cookieAlertCloseButton }}
            </div>
        </div>
        {{/ cookiealert }}
    </div>
    <header id="page-top">
        <div class="top-header">
            <div class="global-logo"><a href="{{ url }}" class="brandlogo-wrapper"> {{#field_logo_background}}<img
                    class="{{styleModifier}}" src="{{url}}" alt="{{alt}}"/> {{/field_logo_background}}
                {{#field_image}}<img class="{{styleModifier}}" src="{{url}}" alt="{{alt}}"/> {{/field_image}}</a></div>
            <span class="brandSocialLinks {{styleModifier}}">  <ul class="brandSocialLinks-list">{{#field_social_links}}
                <li><a href="{{ field_url.link_url }}" title="{{field_url.link_title}}" target="{{field_target}}"><i
                        class="fa {{field_icon}}"></i></a></li>
                {{/field_social_links}}
            </ul></span>
            <div class="nav-toggle hide-for-large-up"><span></span></div>
            <div class="cf"></div>
        </div>
        <nav class="primarynav hide-for-medium-down">
            <form class="primarynav-globalsearch" action="{{config.lbl_search_form_action}}">
                <div class="globalsearch"><input id="{{id}}" type="text" name="{{config.lbl_search_field_name}}"
                                                 placeholder="{{config.lbl_search_placeholder}}"/> <i
                        class="fa fa-search"></i></div>
            </form>
            <ul class="primarynav-menu"> {{#field_menu_link}}
                <li class="primarynav-menuitem {{#field_sub_links.0}}nav-dropdown{{/field_sub_links.0}}">
                    {{#field_parent_link}} <a class="primarynav-menulink" href="{{titel}}">{{title}}
                    {{#field_sub_links.0}}<i class="fa fa-caret fa-lg"></i>{{/field_sub_links.0}}</a>
                    {{/field_parent_link}}
                    <ul class="primarynav-submenu"> {{#field_sub_links}}
                        <li class="primarynav-submenuitem"><a class="primarynav-submenulink"
                                                              href="{{title}}">{{title}}</a></li>
                        {{/field_sub_links}}
                    </ul>
                </li>
                {{/field_menu_link}}
            </ul>
            <div class="primarynav-languageselect">
                <div class="primarynav-menuitem nav-dropdown"> {{#field_language_selector_links}} {{#field_parent_link}}
                    <a class="primarynav-menulink" href="#">{{title}} <i class="fa fa-caret fa-lg"></i></a>
                    {{/field_parent_link}}
                    <ul class="primarynav-submenu"> {{#field_sub_links}}
                        <li class="primarynav-submenuitem"><a class="primarynav-submenulink" href="{{uri}}"
                                                              alt="{{title}}">{{title}}</a></li>
                        {{/field_sub_links}}
                    </ul>
                    {{/field_language_selector_links}}
                </div>
            </div>
        </nav>
    </header>
    <main> {{# first-promo }}
        <section class="herocarousel"> {{# heroitems}}
            <div class="herocarousel-item"> {{#img}}<img class="{{styleModifier}}" src="{{url}}" alt="{{alt}}"/>
                {{/img}} {{#heading}}
                <div class="herocarousel-item-text"><h2 class="{{styleModifier}}">{{heading}}</h2> {{#description}}<p
                        class="{{styleModifier}}">{{ description }}</p> {{/description}}
                    <button class="btn {{ styleModifier }}" type="button" name="{{ buttonName }}">{{ buttonText }}
                    </button>
                </div>
                {{/heading}}
            </div>
            {{/ heroitems}}
        </section>
        {{/ first-promo }} {{# contentCarouselHeader }}
        <section class="section-content-carousel">
            <div class="contentwrapper">
                <div class="margin-standard-horizontal">
                    <div class="carouselheader"><h3 class="{{styleModifier}}">{{heading}}</h3></div>
                    <div class="carousel"> {{# recipelist }}
                        <div class="contenttilecontainer">
                            <div class="contenttile"><a href="{{ href }}"> {{#img}} <img class="contenttile-image"
                                                                                         src="{{src}}" alt="{{alt}}"/>
                                {{/img}}
                                <div class="contenttile-title"> {{title}}</div>
                                {{# label}}
                                <div class="contenttile-label"> {{label.text}}</div>
                                {{/ label}} {{# category}}
                                <div class="contenttile-category">
                                    <div>{{ category }}</div>
                                </div>
                                {{/ category}} </a> {{# teaser}}
                                <div class="contenttile-teaser"> {{teaser}}</div>
                                {{/ teaser}} {{# call-to-action}}
                                <div class="contenttile-cta"><a href="{{href}}">Read More</a></div>
                                {{/ call-to-action}}
                            </div>
                        </div>
                        {{/ recipelist }}
                    </div>
                </div>
            </div>
        </section>
        {{/ contentCarouselHeader }} {{# second-promo }}
        <section class="promocontainer"> {{# promoitems}}
            <div class="promocontainer-item"> {{#img}}<img class="{{styleModifier}}" src="{{url}}" alt="{{alt}}"/>
                {{/img}} {{#promotext}}
                <div class="promocontainer-item-text"><h2 class="{{styleModifier}}">{{heading}}</h2>
                    <p class="{{styleModifier}}">{{ description }}</p> {{# contentPromoButton }}
                    <button class="btn {{ styleModifier }}" type="button" name="{{ buttonName }}">{{ buttonText }}
                    </button>
                    {{/ contentPromoButton }}
                </div>
                {{/promotext}}
            </div>
            {{/ promoitems}}
        </section>
        {{/ second-promo }} {{# productCarouselHeader }}
        <section class="carouselwrapper">
            <div class="contentwrapper">
                <div class="margin-standard-horizontal">
                    <div class="carouselheader"><h2 class="{{styleModifier}}">{{heading}}</h2></div>
                    <div class="carousel"> {{# productlisting }}
                        <div class="productlistingtile-container">
                            <div class="productlistingtile"> {{#img}}<img class="{{styleModifier}}" src="{{url}}"
                                                                          alt="{{alt}}"/> {{/img}}
                            </div>
                            <span class="producttile-title">{{title}}</span></div>
                        {{/ productlisting }}
                    </div>
                </div>
            </div>
        </section>
        {{/ productCarouselHeader }}
    </main>
    <footer class="footer">
        <div class="footer-buttonwrapper"><a href="#page-top" class="btn-movetotop"> Top <i
                class="fa fa-long-arrow-up"></i></a></div>
        <div class="footer-main">
            <div class="contentwrapper">
                <div class="footer-main-wrapper">
                    <div class="footer-main-upper">
                        <div class="logowrapper"><a href="{{ url }}"> {{#asr-logo.img}}<img class="{{styleModifier}}"
                                                                                            src="{{url}}"
                                                                                            alt="{{alt}}"/>
                            {{/asr-logo.img}}</a></div>
                        <div class="linkwrapper flex-row">
                            <div class="footer-navigation"> {{# footer-links}}
                                <div class="navigation-list"><p><a href="{{url}}">{{category}}</a></p>
                                    <ul> {{# sublinks }}
                                        <li><a href="{{url}}">{{text}}</a></li>
                                        {{/ sublinks}}
                                    </ul>
                                </div>
                                {{/ footer-links }}
                            </div>
                            <span class="brandSocialLinks {{styleModifier}}">  <ul class="brandSocialLinks-list">
                                {{#field_social_links}}
                                <li><a href="{{ field_url.link_url }}" title="{{field_url.link_title}}"
                                       target="{{field_target}}"><i class="fa {{field_icon}}"></i></a></li>
                                {{/field_social_links}}
                            </ul></span></div>
                    </div>
                    <div class="footer-main-lower">
                        <ul class="nav-footer"> {{#horizontalLinkgroup}}
                            <li><a href="{{ url }}">{{linkname}}</a></li>
                            {{/horizontalLinkgroup}}
                        </ul>
                        <p class="copyright">{{{ copyrightText }}}</p></div>
                </div>
            </div>
        </div>
    </footer>
</div>